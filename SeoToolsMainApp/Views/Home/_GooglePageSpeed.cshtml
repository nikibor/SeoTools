@model SeoToolsMainApp.Api.GooglePageSpeedApi

<div class="card">
    <div class="card-header">
        <h3>Оценка Google Page Speed:</h3>
    </div>
    <div class="card-block">    
        <ul class="list-group">
            <li class="list-group-item">&lt;Title&gt; сайта: <strong>@Model.TitleOfSite</strong></li>
            <li class="list-group-item">Скорость загрузки сайта: <strong>@Model.SpeedScore из 100</strong></li>
            <li class="list-group-item">Количество хостов: <strong>@Model.NumberOfHosts</strong></li>
            <li class="list-group-item">Количество статических компонентов: <strong>@Model.NumberStaticOfResources</strong></li>
            <li class="list-group-item">Размер html файла: <strong>@Model.HtmlSize</strong></li>
            <li class="list-group-item">Количество JavaScript файлов: <strong>@Model.NumberJsResources</strong></li>
            <li class="list-group-item">Размер JavaScript файлов: <strong>@Model.JavaScriptSize</strong></li>
            <li class="list-group-item">Количество Css файлов :<strong>@Model.NumberCssResources</strong></li>            
            <li class="list-group-item">Размер Css компонентов: <strong>@Model.CssSize</strong></li>
            <li class="list-group-item">Размер изображений на странице: <strong>@Model.ImageSize</strong></li>            
        </ul>
    </div>    
</div>