@model SeoToolsMainApp.Api.YandexIndex

<div class="card">
    <div class="card-header">
        <h3>Yandex цитирование</h3>
    </div>
    <div class="card-block">
        <ul class="list-group">
            <li class="list-group-item">Индекс цитирования: <strong>@Model.TcyRang</strong></li>
            <li class="list-group-item">Уровень цитирования <strong>@Model.TcyIndex из 6</strong></li>
            <li class="list-group-item">Яндекс католог: <strong>@Model.TextInfo</strong></li>
        </ul>
    </div>
</div>